    <script>
        function mostrarPagina(nome) {
    document.querySelectorAll('.pagina').forEach(div => div.classList.remove('ativa'));

    if (nome === 'inicio') {
        document.getElementById('paginaInicial').classList.add('ativa');
    } else {
        document.getElementById('pagina' + nome.charAt(0).toUpperCase() + nome.slice(1)).classList.add('ativa');
    }
}
    </script>

    <script>
    function mostrarPagina(nome) {
        document.querySelectorAll('.site > div').forEach(div => {
            div.style.display = 'none';
        });

        if (nome === 'inicio') {
            document.getElementById('INICIO').style.display = 'block';
        } else {
            const id = 'pagina' + nome.charAt(0).toUpperCase() + nome.slice(1);
            document.getElementById(id).style.display = 'block';
        }
    }
    window.onload = function() {
        mostrarPagina('inicio');
    };
    </script>

    
